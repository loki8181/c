#include <stdio.h>

int main() {
    int m1, m2, m3, m4, m5;
    printf("Enter marks for 5 subjects:\n");
    scanf("%d", &m1);
    scanf("%d", &m2);
    scanf("%d", &m3);
    scanf("%d", &m4);
    scanf("%d", &m5);
    int sum = m1 + m2 + m3 + m4 + m5;
    int avg = sum / 5;
    printf("Sum = %d, Average = %d\n", sum, avg);
    if (avg > 90 && avg <= 100) {
        printf("A Grade\n");
    } else if (avg > 80 && avg <= 90) {
        printf("B Grade\n");
    } else if (avg > 70 && avg <= 80) {
        printf("C Grade\n");
    } else if (avg > 60 && avg <= 70) {
        printf("D Grade\n");
    } else {
        printf("F Grade\n");
    }

    return 0;
}
